

#include<stdio.h>
int main()
{
   int n,xor,i;
   scanf("%d",&n);
   if(n==1)
   {
       scanf("%d",&n);
    printf("%d\n",n);
   }
   else if(n>1)
   {
       int a[n];
       scanf("%d",&a[0]);
       xor=a[0];
       for(i=1;i<n;i++)
       {
           scanf("%d",&a[i]);
           xor=xor^a[i];
           
       }
       printf("%d",xor);
   }
   return(0);

}

